
%  .bn1 pull letter from upper tray on the UNAP9210.
/ibmul {statusdict begin 0 setpapertray 
	/letter trayswitch false end
	/pgsize 11 inch def} def

%  .bn2 pull letter from lower tray on the UNAP9210.
/ibmll {statusdict begin 1 setpapertray 
	/letter trayswitch false end
	/pgsize 11 inch def} def

%  .bn3 pull legal from upper tray on the UNAP9210.
/ibmulg {statusdict begin 0 setpapertray 
	/legal trayswitch false end
	/pgsize 14 inch def} def

%  .bn4 pull legal from lower tray on the UNAP9210.
/ibmllg {statusdict begin 1 setpapertray 
	/legal trayswitch false end
	/pgsize 14 inch def} def

%  .bn5 pull letter continuous on the UNAP9210.
/ibmcl {statusdict begin 0 setpapertray letter
	/1 setpapertray letter trayswitch true end
	/pgsize 11 inch def} def

%  .bn6 pull legal continuous on the UNAP9210.
/ibmclg {statusdict begin 1 setpapertray legal
	/1 setpapertray legal trayswitch true end
	/pgsize 14 inch def} def

%  .bn7 pull #10 envelope on the UNAP9210.
/ibm10e {statusdict begin 2 setpapertray 
	/4.125x9.5envelope trayswitch false end
	/pgsize 9.5 inch def} def

%  .bn8 pull euro envelope on the UNAP9210.
/ibmeure {statusdict begin 2 setpapertray 
	/110x220envelope trayswitch false end} def
